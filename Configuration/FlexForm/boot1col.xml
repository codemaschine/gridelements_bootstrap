<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <meta type="array">
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <container>
            <ROOT type="array">
                <TCEforms>
                    <sheetTitle>Container</sheetTitle>
                </TCEforms>
                <el type="array">

                    <partial>
                        <TCEforms type="array">
                            <label>Modul</label>
                            <config>
                                <type>select</type>
                                <itemsProcFunc>SaschaEnde\GridelementsBootstrap\Flexform\Bootstrap->getModules</itemsProcFunc>
                                <size>1</size>
                                <minitems>1</minitems>
                                <maxitems>1</maxitems>
                            </config>
                        </TCEforms>
                    </partial>


                    <container>
                        <TCEforms type="array">
                            <label>Modul Container CSS</label>
                            <config>
                                <type>select</type>
                                <renderType>selectMultipleSideBySide</renderType>
                                <itemsProcFunc>SaschaEnde\GridelementsBootstrap\Flexform\Bootstrap->getModuleCss</itemsProcFunc>
                                <size>10</size>
                                <minitems>1</minitems>
                                <maxitems>9999</maxitems>
                            </config>
                        </TCEforms>
                    </container>


                </el>
            </ROOT>
        </container>

    </sheets>
</T3DataStructure>